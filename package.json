    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "css-loader": "^0.28.11",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mongoose": "^5.2.4",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.3",
    "postcss-loader": "^2.1.5",
    "style-loader": "^0.21.0",
